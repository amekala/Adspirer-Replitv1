Date: Sun, 23 Mar 2025 16:45:18 +0000
Subject: [PATCH 11/13] Fix: Correct welcome message behavior in chat; remove
 redundant initial message request.

Replit-Commit-Author: Agent
Replit-Commit-Session-Id: 85c1a14b-52fa-434d-a47c-c58770e501da
Replit-Commit-Screenshot-Url: https://storage.googleapis.com/screenshot-production-us-central1/974fb22c-7bf2-435f-92b8-6b8a9968e57b/dfece6a3-e98e-4323-ba5e-efc0aad181e5.jpg
---
 client/src/pages/chat.tsx | 21 +++------------------
 server/routes.ts          |  8 ++++----
 2 files changed, 7 insertions(+), 22 deletions(-)

diff --git a/client/src/pages/chat.tsx b/client/src/pages/chat.tsx
index 91cfae5..7022450 100644
--- a/client/src/pages/chat.tsx
+++ b/client/src/pages/chat.tsx
@@ -112,24 +112,9 @@ export default function ChatPage() {
             conversationWithTyping
           );
           
-          // Now send a request to get an actual welcome message
+          // The server automatically generates a welcome message when creating a conversation
+          // We just need to refresh to get the server-generated welcome message
           try {
-            const completionResponse = await fetch('/api/chat/completions', {
-              method: 'POST',
-              headers: {
-                'Content-Type': 'application/json',
-              },
-              body: JSON.stringify({
-                conversationId: newConversation.id,
-                initialMessage: true
-              }),
-              credentials: 'include'
-            });
-            
-            if (!completionResponse.ok) {
-              console.error('Failed to get initial welcome message');
-            }
-            
             // Final refresh to ensure we have the welcome message
             queryClient.invalidateQueries({ 
               queryKey: ["/api/chat/conversations", newConversation.id] 
@@ -139,7 +124,7 @@ export default function ChatPage() {
               queryKey: ["/api/chat/conversations", newConversation.id, "specific"] 
             });
           } catch (error) {
-            console.error("Error getting welcome message:", error);
+            console.error("Error refreshing conversation data:", error);
           }
         }, 500);
       } catch (error) {
diff --git a/server/routes.ts b/server/routes.ts
index 934143f..a4fb5d0 100644
--- a/server/routes.ts
+++ b/server/routes.ts
@@ -912,11 +912,11 @@ export async function registerRoutes(app: Express): Promise<Server> {
     
     console.log('User authenticated:', req.user.id);
 
-    const { conversationId, message, initialMessage } = req.body;
+    const { conversationId, message } = req.body;
     
-    // Allow an exception for initial welcome messages
-    if (!conversationId || (!message && !initialMessage)) {
-      return res.status(400).json({ message: "Conversation ID and message (or initialMessage flag) are required" });
+    // Require both conversation ID and message
+    if (!conversationId || !message) {
+      return res.status(400).json({ message: "Conversation ID and message are required" });
     }
 
     try {
